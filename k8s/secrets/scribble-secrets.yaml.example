# Scribble Secrets Template
# Copy this file to scribble-secrets.yaml and fill in actual values
# DO NOT commit the actual secrets file to version control
#
# Generate admin secret with: openssl rand -base64 32
---
apiVersion: v1
kind: Secret
metadata:
  name: scribble-secrets
  namespace: default
  labels:
    app: scribble
type: Opaque
stringData:
  admin-secret: "REPLACE_WITH_ACTUAL_SECRET"
